{
  "name": "node-test",
  "version": "1.0.0",
  "description": "Node & React-Redux Boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/homanbromand/node-react-bootstrap.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "DEBUG_FD=3 DEBUG=PROD node ./server 3> ./server/production.log",
    
    "sym:remove": "", 
    "sym:add": "",
    
    "dev": "npm run sym:remove && npm run sym:add && DEBUG=DEV,PROD nodemon",

    "build:dev": "NODE_ENV=development webpack --colors --config webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --colors --config webpack.config.js",
    
    "db:rollback": "knex migrate:rollback --knexfile server/knexfile.js",
    "db:migrate": "knex migrate:latest --knexfile server/knexfile.js",
    "db:seed": "knex seed:run --knexfile server/knexfile.js",
    "db:boot": "npm run db:rollback && npm run db:migrate && npm run db:seed"
  },
  "keywords": [
    "node",
    "react",
    "boilerplate"
  ],
  "author": "Homan Bromand",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3",
    "babel": "^6.5.2",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "babili": "^0.0.9",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.2",
    "connect-redis": "^3.2.0",
    "css-loader": "^0.26.1",
    "debug": "^2.3.3",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "history": "^4.5.0",
    "hjs": "0.0.6",
    "knex": "^0.12.6",
    "lodash": "^4.17.2",
    "mysql": "^2.12.0",
    "node-sass": "^3.13.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redis": "^2.6.3",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-promise-middleware": "^4.2.0",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.2",
    "serve-favicon": "^2.3.2",
    "style-loader": "^0.13.1",
    "validatorjs": "^3.11.0"
  },
  "devDependencies": {
    "extract-text-webpack-plugin": "^1.0.1",
    "nodemon": "1.11.0",
    "webpack": "^1.14.0"
  }
}
